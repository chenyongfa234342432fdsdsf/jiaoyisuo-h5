{
  "locales": [
    "en-US",
    "zh-CN",
    "zh-HK",
    "ta-IN",
    "hi-IN",
    "pt-BR",
    "vi-VN",
    "ko-KR",
    "id-ID",
    "th-TH",
    "ja-JP"
  ],
  "sourceLocale": "zh-CN",
  "compileNamespace": "ts",
  "catalogs": [
    {
      "path": "src/locales/{locale}/messages",
      "include": [
        "src",
        "renderer"
      ]
    }
  ],
  "orderBy": "origin",
  "formatOptions": {
    "origins": false,
    "lineNumbers": false
  },
  "format": "minimal",
  "fallbackLocales": {
    "default": "en-US"
  },
  "extractBabelOptions": {
    "presets": [
      "@babel/preset-typescript"
    ]
  }
}
